    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "cwd": "${workspaceFolder}/src",
            "env": {
                "HTTPS_PROXY": "",
                "HTTP_PROXY": ""
            },
            "args": [
                "main:app",
                "--reload"
            ],
            "jinja": false,
            "justMyCode": false
        }
    ]
}
